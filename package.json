{
    "name": "unrail-engine",
    "version": "0.3.7",
    "description": "A lightweight 2D game engine written in typescript",
    "main": "dist/unrail-engine.umd.js",
    "module": "dist/unrail-engine.es.js",
    "files": [
        "dist",
        "src"
    ],
    "types": "dist/unrail-engine.d.ts",
    "typescript": {
        "definition": "dist/unrail-engine.d.ts"
    },
    "scripts": {
        "dev": "vite",
        "build": "vite build && npm run build-declaration",
        "build-declaration": "tsc -p .",
        "serve": "vite preview",
        "docs": "npx typedoc src/index.ts src/events/index.ts src/render/index.ts --exclude ./node_modules/",
        "publish-to-npm": "npx auto-version --patch npm run docs; git add .; git commit -m 'publish to NPM'; git push; npm publish"
    },
    "dependencies": {
        "@dorianb/random-js": "^0.0.4",
        "file-saver": "^2.0.5",
        "stats.js": "^0.17.0",
        "svg-path-builder": "^1.0.2"
    },
    "devDependencies": {
        "@types/offscreencanvas": "^2019.6.2",
        "rollup-plugin-visualizer": "^5.5.0",
        "auto-version-js": "^0.3.7",
        "typedoc": "^0.20.27",
        "typescript": "^4.2.3",
        "vite": "^2.0.2"
    },
    "keywords": [
        "game-engine",
        "engine",
        "lightweight",
        "typescript"
    ],
    "author": "d0rianb<dorian.beauchesne@icloud.com>",
    "license": "MIT"
}