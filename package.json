{
    "name": "unrail-engine",
    "version": "0.4.9",
    "description": "A lightweight 2D game engine written in typescript",
    "main": "./dist/unrail-engine.umd.js",
    "module": "./dist/unrail-engine.es.js",
    "files": [
        "dist",
        "src"
    ],
    "types": "dist/unrail-engine.d.ts",
    "scripts": {
        "dev": "vite .",
        "build": "vite build && npm run build-declaration && npm run docs",
        "build-declaration": "rollup -c",
        "serve": "vite preview",
        "docs": "npx typedoc src/index.ts src/events/index.ts src/render/index.ts --exclude ./node_modules/",
        "publish-to-npm": "npm run build && npx auto-version --patch && npm run docs && git add . && git tag -a 'v'$npm_package_version -m 'Version '$npm_package_version && git commit -m 'Publish '$npm_package_version' to NPM'",
        "postpublish": "git push origin --all; git push origin --tags",
        "test": "echo 'v'$npm_package_version"
    },
    "dependencies": {
        "@dorianb/random-js": "^0.0.4",
        "stats.js": "^0.17.0",
        "svg-path-builder": "^1.0.2"
    },
    "devDependencies": {
        "@rollup/plugin-alias": "^3.1.2",
        "@rollup/plugin-commonjs": "^19.0.0",
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/plugin-node-resolve": "^13.0.0",
        "@types/offscreencanvas": "^2019.6.2",
        "rollup-plugin-dts": "^3.0.2",
        "rollup-plugin-esbuild": "^4.2.3",
        "rollup-plugin-json": "^4.0.0",
        "rollup-plugin-visualizer": "^5.5.0",
        "auto-version-js": "^0.3.7",
        "typedoc": "^0.20.27",
        "typescript": "^4.2.3",
        "vite": "^2.0.2"
    },
    "keywords": [
        "game-engine",
        "game",
        "engine",
        "2D",
        "lightweight",
        "typescript"
    ],
    "author": "d0rianb<dorian.beauchesne@icloud.com>",
    "license": "MIT"
}