{
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "moduleResolution": "node",
        "outDir": "./dist",
        "allowJs": true,
        "strict": false,
        "sourceMap": true,
        "importHelpers": true,
        "isolatedModules": true,
        "noEmit": true,
        "esModuleInterop": true,
        "resolveJsonModule": true
    }
}