{
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "moduleResolution": "node",
        "outDir": "./dist",
        "lib": ["webworker", "es2016", "scripthost", "DOM"],
        "skipLibCheck": true,
        "allowJs": true,
        "strict": false,
        "sourceMap": true,
        "importHelpers": true,
        "isolatedModules": false,
        "noEmit": true,
        "esModuleInterop": true,
        "resolveJsonModule": true
    }
}